{
  "name": "clientlinker",
  "version": "7.1.0",
  "description": "Linker all clients whether rpc, addon, http request, mock data, local file ...",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm ls",
    "start": "node ./bin/clientlinker_cli.js run ./test/conf/clientlinker.conf.js",
    "lint": "eslint .",
    "test": "cross-env DEBUG=clientlinker* mocha test/test_*",
    "test-cov": "istanbul cover _mocha -- test/test_* --reporter dot",
    "test-travis": "istanbul cover _mocha --report lcovonly -- test/test_* --reporter dot"
  },
  "bin": {
    "clientlinker": "bin/clientlinker_cli.js"
  },
  "keywords": [
    "client",
    "linker",
    "proxy",
    "rpc",
    "httpproxy"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Bacra/node-clientlinker.git"
  },
  "bugs": {
    "url": "https://github.com/Bacra/node-clientlinker/issues"
  },
  "homepage": "https://github.com/Bacra/node-clientlinker#readme",
  "author": "Bacra <bacra.woo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.2",
    "chalk": "^1.1.3",
    "commander": "2.9.0",
    "debug": "^3.1.0",
    "depd": "^1.1.2",
    "is-promise": "^2.1.0",
    "lodash": "^4.17.10",
    "table": "3.7.9"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "clientlinker-flow-confighandler": "^1.0.1",
    "clientlinker-flow-confighandler-test": "^1.0.1",
    "clientlinker-flow-pkghandler": "^1.0.0",
    "cross-env": "^2.0.0",
    "eslint": "^5.0.1",
    "eslint-config-brcjs": "^0.2.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.3"
  },
  "optionalDependencies": {
    "nan-async-example": "^0.1.2"
  }
}
