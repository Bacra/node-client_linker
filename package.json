{
  "name": "clientlinker",
  "version": "5.1.1",
  "description": "Linker all clients whether rpc, addon, http request, mock data, local file ...",
  "main": "index.js",
  "scripts": {
	"prepublish": "npm ls",
    "start": "node ./bin/clientlinker_cli.js ./test/clientlinker.conf.js",
    "test": "cross-env DEBUG=clientlinker* mocha test/test_*",
    "test-cov": "istanbul cover ./node_modules/.bin/_mocha -- test/test_* --reporter dot",
    "test-travis": "istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- test/test_* --reporter dot"
  },
  "bin": {
    "clientlinker": "bin/clientlinker_cli.js"
  },
  "keywords": [
    "client",
    "linker",
    "proxy"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Bacra/node-clientlinker.git"
  },
  "bugs": {
    "url": "https://github.com/Bacra/node-clientlinker/issues"
  },
  "homepage": "https://github.com/Bacra/node-clientlinker#readme",
  "author": "Bacra <bacra.woo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^2.0.0",
    "expect.js": "^0.3.1",
    "express": "^4.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2"
  },
  "dependencies": {
    "bluebird": "^3.3.1",
    "debug": "^2.2.0",
    "depd": "^1.1.0",
    "is-promise": "^2.1.0",
    "jsonk": "^0.1.2",
    "mkdirp": "^0.5.1",
    "nopt": "^3.0.6",
    "raw-body": "^2.1.7",
    "request": "^2.69.0",
    "table": "^3.7.8",
    "underscore": "^1.8.3"
  },
  "optionalDependencies": {
    "nan-async-example": "^0.1.1"
  }
